{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "web-build",
  "framework": "remix",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}